#!/bin/sh
#
# Udskriv alle overståede Eggs der endnu ikke er blevet bogført.
source $CONCIEGGS_DIR/eggspi.lib

prev=$(prevEggs)
if [ "$prev" != "" ]; then
    prevEggs | while read eggs; do
        echo "Der var Eggs $(timeToDate $eggs), med $(describeEggs $eggs) tilmeldt."
    done
else
    echo "Alle Eggs er blevet bogført!"
fi
