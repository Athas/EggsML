#!/bin/sh
#
# Udskriv alle kommende Eggs med tilmeldte deltagere.


next=$(nextEggs)
if [ "$next" != "" ]; then
    nextEggs | while read eggs; do
        echo "Der er $(countEggsers $eggs) tilmeldt til $(titleEggs $eggs): $(describeEggs $eggs)."
    done
else
    echo "Ingen har tilmeldt sig eggs!"
fi
