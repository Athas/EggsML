#!/bin/sh

next=$(nextEggs)
if [ "$next" != "" ]; then
    nextEggs | while read eggs; do
        echo "Der er Eggs $(timeToDate $eggs), med "
        cat $CONCIEGGS_DB_DIR/eggs/$eggs | while read who; do
            echo $(randomName "$who")
        done | commasize
        echo "tilmeldt."
    done
else
    echo "Ingen har tilmeldt sig eggs!"
fi
