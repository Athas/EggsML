#!/bin/sh
#
# Udskriv alle kommende Eggs med tilmeldte deltagere.
source $CONCIEGGS_DIR/eggspi.lib

next=$(nextEggs)
if [ "$next" != "" ]; then
    nextEggs | while read eggs; do
        echo "Der er Eggs $(timeToDate $eggs), med $(describeEggs $eggs) tilmeldt."
    done
else
    echo "Ingen har tilmeldt sig eggs!"
fi
