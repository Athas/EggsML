#!/bin/sh
#
# Den er god den her - men du m√• sandsynligvis ikke bruge den.

# Skal indeholde en nylinje-separeret liste af autoriserede IRC-nicks.
authfile="$HOME/.concieggs-secret-auth"

if ! [ -f "$authfile" ]; then
    touch "$authfile"
fi

if grep -qE "^$(authedName)$" $authfile; then
    sh -c "$*"
else
    echo "$EGGS_USER: Beklager, men du er ikke stor nok endnu."
fi
