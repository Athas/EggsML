#!/bin/sh

hjulene=`randomElement <<EOF
hjulende
Illuminati
menneskeheden
dyret i os
dataloger
fysikere
biologer
EOF`

drejer=`randomElement <<EOF
drejer
styrer
leder verden
komplementerer bits
sover
spiser giftig kage
grinte ondt
taster
vader rundt
tilbad hedenske guder
EOF`

echo $hjulene $drejer
